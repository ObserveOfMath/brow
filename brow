if [[ $1 = "-ht" ]]; then
	curl -s $(
		echo "https://html.duckduckgo.com/html/?q=how%20to%20$2" |
		python "$HOME/Projects/brow/url.py") |
		tail -n +200 | python "$HOME/Projects/brow/brow.py"
	exit
fi

curl -s $(
	echo "https://html.duckduckgo.com/html/?q=$1" |
	python "$HOME/Projects/brow/url.py") |
	tail -n +200 | python "$HOME/Projects/brow/brow.py"
